    "slate-auto-replace": "^0.12.1",
    "slate-collapse-on-escape": "^0.8.1",
    "slate-html-serializer": "^0.7.36",
    "slate-no-empty": "^0.1.0",
    "slate-paste-linkify": "^0.7.0",
    "slate-react": "^0.21.18",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.2.2",
    "typescript": "3.4.1",
    "url-loader": "1.1.2",
    "webpack": "4.28.3",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "3.1.14",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.3"
  },
  "scripts": {
    "start": "node scripts/frontend_start.js",
    "build": "node scripts/frontend_build.js",
    "test": "node scripts/frontend_test.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/is-hotkey": "^0.1.1",
    "@types/lodash": "^4.14.123",
    "@types/reach__router": "^1.2.3",
    "@types/slate": "^0.44.2",
    "@types/slate-html-serializer": "^0.6.3",
    "@types/slate-react": "^0.21.2",
    "react-hot-loader": "^4.8.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/assets/js/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/assets/js/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "./node_modules/jest-watch-typeahead/filename.js",
      "./node_modules/jest-watch-typeahead/testname.js"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}